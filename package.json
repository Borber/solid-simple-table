{
  "name": "solid-simple-table",
  "version": "0.0.1",
  "description": "React Table Component done right",
  "main": "dist/SimpleTable.js",
  "module": "dist/SimpleTable.module.js",
  "types": "dist/SimpleTable.d.ts",
  "source": "src/SimpleTable.tsx",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "clean": "shx rm -rf dist .parcel-cache",
    "dev": "cross-env NODE_ENV=development parcel watch src/SimpleTable.tsx",
    "build": "cross-env NODE_ENV=production parcel build src/SimpleTable.tsx",
    "prepare": "npm run build",
    "test": "cross-env BABAEL_CJS=true jest",
    "demo.simple": "npm run build && cross-env NODE_ENV=production parcel serve ./demo/simple/index.html --dist-dir ./demo/simple/dist --open",
    "demo.custom-sort": "npm run build && cross-env NODE_ENV=production parcel serve ./demo/custom-sort/index.html --dist-dir ./demo/custom-sort/dist --open"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@parcel/babel-plugin-transform-runtime": "^2.0.0-nightly.1823",
    "@parcel/babel-preset-env": "^2.0.0-alpha.3",
    "@parcel/validator-typescript": "^2.0.0-alpha.3",
    "@testing-library/dom": "^7.16.1",
    "@testing-library/jest-dom": "^5.10.1",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.16",
    "babel-jest": "^26.6.3",
    "babel-preset-solid": "^0.23.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-config-atomic": "^1.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "less": "^4.0.0",
    "parcel": "^2.0.0-nightly.489",
    "shx": "^0.3.3",
    "solid-js": "^0.23.4",
    "typescript": "4.1.3"
  },
  "targets": {
    "main": {
      "context": "browser",
      "engines": {
        "browsers": "> 0.25%"
      },
      "includeNodeModules": true,
      "isLibrary": true
    },
    "module": {
      "context": "browser",
      "engines": {
        "browsers": "Chrome 76"
      },
      "includeNodeModules": false,
      "isLibrary": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aminya/solid-simple-table.git"
  },
  "author": "Amin Yahyaabadi <aminyahyaabadi74@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aminya/solid-simple-table/issues"
  },
  "homepage": "https://github.com/aminya/solid-simple-table#readme"
}
